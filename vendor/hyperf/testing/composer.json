{
    "name": "hyperf/testing",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "testing"
    ],
    "description": "Testing for hyperf",
    "autoload": {
        "psr-4": {
            "Hyperf\\Testing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Testing\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.0",
        "psr/container": "^1.0|^2.0",
        "phpunit/phpunit": "^9.5",
        "hyperf/contract": "~3.0.0",
        "hyperf/http-message": "~3.0.0",
        "hyperf/http-server": "~3.0.0",
        "hyperf/utils": "~3.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "bin": [
        "co-phpunit"
    ]
}
